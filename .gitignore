# Root level .gitignore for ali_web project

# Node modules (for all subfolders)
admin/node_modules/
frontend/node_modules/
backend/node_modules/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# dotenv environment files
admin/.env
frontend/.env
backend/.env

# Build output
admin/build/
frontend/build/
backend/build/

# Coverage
coverage/

# IDE files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# OS files
.DS_Store
Thumbs.db

# Misc
*.local

# Optional: if using Linux or Mac hidden files
.*.swp

# Backup files
*~
